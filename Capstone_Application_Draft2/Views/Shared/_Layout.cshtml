<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title></title>
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900" rel="stylesheet" />
    <link href="~/Content/default.css" rel="stylesheet" />
    <link href="~/Content/fonts.css" rel="stylesheet" />
    <!--[if IE 6]><link href="default_ie6.css" rel="stylesheet" type="text/css" /><![endif]-->

</head>
<body>
    <div id="header">
        <div id="menu" class="container">
            <ul>
                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                <li>@Html.ActionLink("About", "About", "Home")</li>
                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                <li>@Html.Partial("_LoginPartial")</li>
                @if (Request.IsAuthenticated)
                {
                    <li>@Html.ActionLink("My List", "Index", "Custom_User_List")</li>
                }
            </ul>

        </div>
    </div>

    <div class="container main-body">
        @RenderBody()
    </div>

    <div id="copyright" class="container">
        <p>&copy; @DateTime.Now.Year Weddington. All rights reserved. | Photos by <a href="http://fotogrph.com/">Fotogrph</a> | Design by <a href="http://templated.co" rel="nofollow">TEMPLATED</a>.</p>
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/jsPDF-1.3.2/jspdf.js"></script>
    <script src="~/Scripts/jsPDF-1.3.2/libs/html2pdf.js"></script>
    <script src="~/Scripts/jsPDF-1.3.2/plugins/canvas.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
