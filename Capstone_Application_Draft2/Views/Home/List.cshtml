@{
    ViewBag.Title = "Lists";
}

<div class="container">
    <div class="header">
        <div class="clear">
            <i class="fa fa-refresh" aria-hidden="true"></i>
        </div>
        <div id="date"></div>
    </div>
    <div class="content">
        <ul id="list"></ul>
    </div>
    <div class="add-to-do">
        <i class="fa fa-plus-circle" aria-hidden="true"></i>
        <input type="text" id="item" placeholder="Add a to do"
    </div>
</div>